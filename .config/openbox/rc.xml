    <!-- Switch workspaces with W-# -->
    <keybind key="W-1">
      <action name="GoToDesktop">
        <to>1</to>
      </action>
    </keybind>
    <keybind key="W-2">
      <action name="GoToDesktop">
        <to>2</to>
      </action>
    </keybind>
    <keybind key="W-3">
      <action name="GoToDesktop">
        <to>3</to>
      </action>
    </keybind>
    <keybind key="W-4">
      <action name="GoToDesktop">
        <to>4</to>
      </action>
    </keybind>
    <keybind key="W-5">
      <action name="GoToDesktop">
        <to>5</to>
      </action>
    </keybind>
    <!--Send window to another workspace -->
    <keybind key="S-W-1">
      <action name="SendToDesktop">
        <desktop>1</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-2">
      <action name="SendToDesktop">
        <desktop>2</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-3">
      <action name="SendToDesktop">
        <desktop>3</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-4">
      <action name="SendToDesktop">
        <desktop>4</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-5">
      <action name="SendToDesktop">
        <desktop>5</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-7">
      <action name="GoToDesktop">
        <to>1</to>
      </action>
    </keybind>
    <keybind key="W-8">
      <action name="GoToDesktop">
        <to>2</to>
      </action>
    </keybind>
    <keybind key="W-9">
      <action name="GoToDesktop">
        <to>3</to>
      </action>
    </keybind>
    <keybind key="W-0">
      <action name="GoToDesktop">
        <to>4</to>
      </action>
    </keybind>
    <!--Send window to another workspace -->
    <keybind key="S-W-7">
      <action name="SendToDesktop">
        <desktop>1</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-8">
      <action name="SendToDesktop">
        <desktop>2</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-9">
      <action name="SendToDesktop">
        <desktop>3</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="S-W-0">
      <action name="SendToDesktop">
        <desktop>4</desktop>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-d">
      <action name="ToggleShowDesktop"/>
    </keybind>
    <!-- Keybindings for windows -->
    <keybind key="W-q">
      <action name="Close"/>
    </keybind>
